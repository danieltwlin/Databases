# 時間比對
# 比對現時間
select count(id) from 表名 Where date_format(from_unixtime(时间字段),'%Y-%m-%d') = date_format(now(),'%Y-%m-%d')

# date_format(now(),'%Y-%m-%d')

SELECT * from tb1 where user_id=1 and lecture_date=  date_format(now(),'%Y-%m-%d')

#加入 Count 和比對條件
SELECT count(id) from tab1 where user_id=1 and lecture_date=  date_format(now(),'%Y-%m-%d') and subject = 'English'


